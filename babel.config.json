{
  "presets": [["@babel/preset-env"], "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "@/public": "./public",
          "@/components": "./src/components",
          "@/pages": "./src/pages",
          "@/styles": "./src/styles"
        }
      }
    ]
  ]
}
